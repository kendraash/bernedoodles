<%= render 'layouts/errors', :object => @puppy %>

<%= form_for [@dog, @litter, @puppy] do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :sex %>
  <%= f.text_field :sex %>
  <%= f.label :weight %>
  <%= f.text_field :weight %>
  <%= f.label :date_of_birth %>
  <%= f.date_field :date_of_birth %>
  <%= f.submit "Submit" %>
<% end %>
